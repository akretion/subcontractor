<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_ecotaxe_res_config_settings" model="ir.ui.view">
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="account.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//div[@id='subcontractor-settings']" position="inside">
                <div class="o_setting_left_pane" />
                <div class="o_setting_right_pane">
                    <div class="row">
                    </div>
                        <label for="prepaid_countdown_journal_id" class="col-md-5" />
                        <field
                        name="prepaid_countdown_journal_id"
                        options="{'no_create': True, 'no_edit': True, 'no_open': True}"
                    />
                </div>
            </xpath>
        </field>
    </record>
</odoo>
